{
  "name": "Pixr",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "release": "npx react-native run-android --variant=release",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "open": "npm start --reset-cache",
    "clean:android": "cd android && ./gradlew clean && cd -",
    "clean:cache": "cd android && ./gradlew cleanBuildCache && cd -",
    "clean:ios": "cd ios && xcodebuild clean && cd -",
    "copy:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "assemble:android": "cd android && ./gradlew clean && ./gradlew assembledebug",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "update:a": "cd android && ./gradlew cleanBuildCache && ./gradlew clean && react-native bundle --platform android --dev false --entry-file index.js --bundle-output app/src/main/assets/index.android.bundle --assets-dest app/src/main/res && ./gradlew clean && ./gradlew assembledebug && cd -"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.14",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-firebase/app": "^13.1.0",
    "@react-native-firebase/auth": "^13.1.0",
    "@react-native-google-signin/google-signin": "^7.0.1",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.5",
    "axios": "^0.24.0",
    "formik": "^2.2.9",
    "lottie-react-native": "^5.0.1",
    "react": "17.0.2",
    "react-native": "0.66.3",
    "react-native-actions-sheet": "^0.5.8",
    "react-native-camera": "^4.2.1",
    "react-native-camera-hooks": "^0.5.3",
    "react-native-document-picker": "^7.1.3",
    "react-native-fast-image": "^8.5.11",
    "react-native-fbsdk": "^3.0.0",
    "react-native-fs": "^2.18.0",
    "react-native-image-crop-picker": "^0.37.2",
    "react-native-image-picker": "^4.3.0",
    "react-native-keyboard-aware-scrollview": "^2.1.0",
    "react-native-modal": "^13.0.0",
    "react-native-paper": "^4.10.1",
    "react-native-phone-number-input": "^2.1.0",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.9.0",
    "react-native-vector-icons": "^9.0.0",
    "react-navigation-stack": "^2.10.4",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/runtime": "^7.16.3",
    "@react-native-community/eslint-config": "^3.0.1",
    "babel-jest": "^27.4.2",
    "eslint": "^8.3.0",
    "jest": "^27.4.2",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-test-renderer": "17.0.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
